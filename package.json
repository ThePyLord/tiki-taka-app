{
  "name": "tiki-taka-app",
  "productName": "tiki-taka-app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "jest",
    "serve": "ts-node server/server.ts"
  },
  "keywords": [],
  "author": {
    "name": "ThePyLord",
    "email": "geraldfeu@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "tiki_taka_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "devContentSecurityPolicy": "default-src 'self' 'unsafe-inline' ws://localhost:5000 data:; script-src 'self' 'unsafe-eval' 'unsafe-inline' data:",
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/scripts/preload.ts"
                  }
                },
                {
                  "html": "./src/init.html",
                  "js": "./src/app/login.ts",
                  "name": "login_window",
                  "preload": {
                    "js": "./src/scripts/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.60",
    "@electron-forge/maker-deb": "^6.0.0-beta.60",
    "@electron-forge/maker-rpm": "^6.0.0-beta.60",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.60",
    "@electron-forge/maker-zip": "^6.0.0-beta.60",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.60",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.13",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "chai-http": "^4.3.0",
    "css-loader": "^6.5.1",
    "dotenv": "^10.0.0",
    "electron": "16.0.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.4.2",
    "jest": "^27.4.0",
    "node-loader": "^2.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "mongoose": "^6.0.14",
    "socket.io": "^4.4.0",
    "ws": "^8.4.0"
  }
}
